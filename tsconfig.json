{
  "compilerOptions": {
    "target": "ESNext",
    "useUnknownInCatchVariables": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "paths": {},
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/**/*.tsx",
    "src/**/*.ts",
    ".next/types/**/*.ts", ],
  "exclude": [
    "node_modules",
    ".next",
    ".out",
    "*.js"
  ],

}
